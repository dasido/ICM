<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="395.0" prefWidth="537.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReceiveIpController">
   <children>
      <Label layoutX="67.0" layoutY="133.0" text="Please enter Server Ip:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="ipTextField" layoutX="218.0" layoutY="129.0" onAction="#receiveIpText" style="-fx-border-color: black;">
         <effect>
            <InnerShadow />
         </effect></TextField>
      <Button fx:id="btnOk" layoutX="393.0" layoutY="129.0" mnemonicParsing="false" onAction="#okAction" prefHeight="25.0" prefWidth="84.0" text="Ok">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label layoutX="143.0" layoutY="47.0" text="Connect to Server:" textFill="#365b35" underline="true">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </Label>
   </children>
   <effect>
      <InnerShadow />
   </effect>
</AnchorPane>
